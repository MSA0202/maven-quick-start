<project
        xmlns="http://maven.apache.org/POM/4.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
		http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>clinic.programmming</groupId>
    <artifactId>example</artifactId>
    <version>1.0</version>

    <!--
    Example maven compiler plugin to add if our maven compiler would not compile our java
    source code
    See Section 4 of read me
    See to get the plugin https://maven.apache.org/plugins/maven-compiler-plugin/examples/set-compiler-source-and-target.html
     -->
    <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.14.1</version>
        <configuration>
          <release>23</release>
        </configuration>
      </plugin>
    </plugins>
    </build>

</project>